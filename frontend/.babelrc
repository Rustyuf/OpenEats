{
    "presets": [
        "es2015",
        "stage-0",
        "react"
    ],
    "plugins": [
        ["react-intl", {
            "messagesDir": "./build/messages/"
        }],
        ["transform-es2015-classes", { "loose": true }],
        "syntax-class-properties",
        "transform-class-properties",
        "transform-object-rest-spread"
    ]
}